<% if @match and ! @match.empty? -%>
[Match]
  <%- if @match['mac_address'] -%>
MACAddress=<%= @match['mac_address'] %>
  <%- end -%>
  <%- if @match['original_name'] -%>
OriginalName=<%= @match['original_name'] %>
  <%- end -%>
  <%- if @match['path'] -%>
Path=<%= @match['path'] %>
  <%- end -%>
  <%- if @match['driver'] -%>
Driver=<%= @match['driver'] %>
  <%- end -%>
  <%- if @match['type'] -%>
Type=<%= @match['type'] %>
  <%- end -%>
  <%- if @match['host'] -%>
Host=<%= @match['host'] %>
  <%- end -%>
  <%- if @match['virtualization'] -%>
Virtualization=<%= @match['virtualization'] %>
  <%- end -%>
  <%- if @match['kernel_command_line'] -%>
KernelCommandLine=<%= @match['kernel_command_line'] %>
  <%- end -%>
  <%- if @match['architecture'] -%>
Architecture=<%= @match['architecture'] %>
  <%- end -%>
<% end %>
